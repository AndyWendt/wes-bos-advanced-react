type User {
  id: ID! @id
  name: String!
  email: String!
}

type Item {
  id: ID! @id
  title: String!
  description: String!
  image: String
  largeImage: String
  price: Int!

#  DateTime is exposed to us via prisma
  createdAt: DateTime @createdAt
  updatedAt: DateTime @updatedAt
#  user: User!
}
